Package: osrm
Type: Package
Title: Interface Between R and the OpenStreetMap-Based Routing Service
        OSRM
Version: 3.5.0
Authors@R: c(person("Timothée", "Giraud", email = "timothee.giraud@cnrs.fr", 
    role = c("cre","aut"),  comment = c(ORCID = "0000-0002-1932-3323")),
