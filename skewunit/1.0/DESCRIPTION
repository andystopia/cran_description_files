Package: skewunit
Type: Package
Title: Estimation and Other Tools for Skew-Unit Models
Version: 1.0
Date: 2025-02-19
Authors@R: c(person("Diego", "Gallardo", role = c("aut","cre"), email = "dgallardo@ubiobio.cl"),
	person("Emilio", "Gomez-Deniz", role = c("aut"), email = "emilio.gomez-deniz@ulpgc.es"),
